<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.jump.serlvet.loginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>com.jump.serlvet.registerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>queryScoreServlet</servlet-name>
        <servlet-class>com.jump.serlvet.queryScoreServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>queryScoreServlet</servlet-name>
        <url-pattern>/queryScore</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addScoreServlet</servlet-name>
        <servlet-class>com.jump.serlvet.addScoreServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addScoreServlet</servlet-name>
        <url-pattern>/addScore</url-pattern>
    </servlet-mapping>
</web-app>